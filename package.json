{
  "name": "vue-currency-symbol",
  "version": "1.0.0",
  "description": "A simple VueJs library that returns symbol of a country.",
  "main": "dist/vue-currency-symbol.umd.js",
  "module": "dist/vue-currency-symbol.esm.js",
  "unpkg": "dist/vue-currency-symbol.min.js",
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-currency-symbol.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-currency-symbol.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-currency-symbol.min.js"
  },
  "author": "Justin Cejowisz Obi",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.11.0",
    "babel-runtime": "^6.9.2",
    "rollup": "^0.66.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-vue": "^4.3.2",
    "vue": "^2.2.1",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^11.1.4",
    "vue-style-loader": "^2.0.3",
    "vue-template-compiler": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cejowisz/vue-currency-symbol.git"
  },
  "keywords": [
    "USA",
    "dollar",
    "Japan",
    "yen",
    "cedis",
    "pounds",
    "currency",
    "Nigeria",
    "Naira",
    "Country",
    "Nodejs",
    "Vue",
    "JavaScript"
  ]
}
